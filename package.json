    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "setup:remix": "npm run setup:remix-debug; npm run setup:remix-lib;",
    "setup:remix-debug": "cd node_modules && rm -rf remix-debug && ln -s ../../remix/remix-debug remix-debug && cd ..",
    "setup:remix-lib": "cd node_modules && rm -rf remix-lib && ln -s ../../remix/remix-lib remix-lib && cd ..",
    "setup:jsvm": "cd node_modules && rm -rf ethereumjs-vm && ln -s ../../ethereumjs-vm ethereumjs-vm && cd ..",
    "postinstall1": "cd node_modules && ln -s @0yi0/ethereumjs-vm ethereumjs-vm && ln -s @0yi0/remix-lib remix-lib && ln -s @0yi0/remix-debug remix-debug && cd .."
  },
